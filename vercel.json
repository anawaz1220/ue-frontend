{
  "name": "urban-ease-frontend",
  "version": 2,
  "framework": "create-react-app",
  "buildCommand": "npm run build:prod",
  "outputDirectory": "build",
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "env": {
    "REACT_APP_ENV": "production",
    "REACT_APP_API_URL": "https://ue-backend-production.up.railway.app",
    "CI": "false"
  }
}