# Frontend API Configuration - Use production Railway URL for local dev
REACT_APP_API_URL=https://ue-backend-production.up.railway.app
REACT_APP_ENVIRONMENT=development

# Development settings
GENERATE_SOURCEMAP=true
FAST_REFRESH=true

# Skip ESLint version conflict
SKIP_PREFLIGHT_CHECK=true

# Fix Node.js v20 compatibility with old react-scripts
NODE_OPTIONS=--openssl-legacy-provider